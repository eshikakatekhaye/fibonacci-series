package fibonacci.series;

import java.util.Scanner;

public class FibonacciSeries {

     
    public static void main(String[] args) {
        int a=0;
        int b=1;
        int c,e;
        System.out.println(" How many numbers of fibonacci series do you want?");
        Scanner s = new Scanner(System.in);
        e=s.nextInt();
        System.out.print(a+"  "+b)   ;
        for(int i=0; i<(e-2); i++)
        {
           c=a+b;
           System.out.print("  "+c);
           a = b;
           b = c;
           
        }
        System.out.println(" ");;
        
        
    }
    
}
